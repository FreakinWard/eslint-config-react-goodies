{
  "name": "@freakin-ward/eslint-config-boilerplate",
  "version": "0.1.1",
  "description": "This repo serves as a boilerplate to configure lint for nextjs apps",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "format": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config prettier.config.js",
    "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config prettier.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FreakinWard/nextjs-boilerplate-lint.git"
  },
  "keywords": [
    "nextjs",
    "lint",
    "eslint"
  ],
  "author": "aaron.l.ward@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FreakinWard/nextjs-boilerplate-lint/issues"
  },
  "homepage": "https://github.com/FreakinWard/nextjs-boilerplate-lint#readme",
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1"
  },
  "peerDependencies": {
    "eslint": ">= 7",
    "eslint-config-prettier": ">= 8",
    "eslint-plugin-import": ">= 2",
    "eslint-plugin-jest": ">= 24",
    "eslint-plugin-prettier": ">= 3",
    "eslint-plugin-simple-import-sort": ">= 7",
    "prettier": ">= 2"
  }
}
